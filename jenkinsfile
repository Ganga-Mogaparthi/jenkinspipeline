pipeline{
    agent {label 'GOL'}
    stages {
        stage('SCM'){
            steps{
                git branch:'master', url:'https://github.com/asquarezone/game-of-life.git'
            }
        }
        stage('Build'){
            steps{
                sh 'mvn clean test'
            }
        }
    }
    post{
        success{
            artifacts '**/*.war'
            junit '**/TEST-*.xml'
        }
    }

}