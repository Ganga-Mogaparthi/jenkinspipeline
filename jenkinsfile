pipeline{
    agent {label 'GOL'}
    stages {
        stage('SCM'){
            steps{
                git branch:'master', url:'https://github.com/asquarezone/game-of-life.git'
            }
        }
        stage('Build'){
            steps{
                sh 'mvn clean test'
            }
             stash includes: '**/*.jar', name: 'goljar'
        }
        stage('QServer'){
            agent {label 'SPC'}
            steps{
                unstash name:'goljar'
            }
        }
    }   

    post{
        success{
            archive '**/*.jar'
            junit '**/TEST-*.xml'
        }
    }

}