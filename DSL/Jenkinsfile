pipeline{
    agent {label 'GOL'}
    stages {
        stage('scm'){
            steps{
                git branch:'master', url:'https://github.com/asquarezone/game-of-life.git'
            }
        }
        stage('build'){
            steps{
                sh 'mvn clean test'
            }
        }
    }
    post{
        success{
            artifacts '**/*.war'
            junit '**/TEST-*.xml'
        }
        failure{

        }
    }
}